@using Mvc_Cv_Project.Models.Entity
@model List<TblDeneyimlerim>
<div class="container-fluid bg-light my-5 py-6" id="service">
    <div class="container-xxl py-6 pb-5" id="team">
        <div class="container">
            <div class="row g-5 mb-5 wow fadeInUp" data-wow-delay="0.1s">
                <div class="col-lg-6">
                    <h1 class="display-5 mb-0">Geliştirdiğim Projeler🧑🏻‍💻</h1>
                </div>
                <div class="col-lg-6 text-lg-end">
                    <a class="btn btn-primary py-3 px-5" href="">Contact Us</a>
                </div>
            </div>
            <div class="row g-4">
                @foreach (var x in Model)
                {


                    <div class="col-lg-4 col-md-6 wow fadeInUp" data-wow-delay="0.3s">
                        <div class="team-item position-relative">
                            <img class="img-fluid rounded" src="/proman-1.0.0/proman-1.0.0/img/team-2.jpg" alt="">
                            <div class="team-text bg-white rounded-end p-4">
                                <div>
                                    <h5>@x.Baslik</h5>
                                    <span>@x.Aciklama</span>
                                </div>
                                <i class="fa fa-arrow-right fa-2x text-primary"></i>
                                <a class="btn btn-lg-square btn-outline-secondary border-2 mx-1" href="@x.ProjeLink" target="_blank"><i class="fa fa-link"></i></a>
                                <a class="btn btn-lg-square btn-outline-secondary border-2 mx-1" href="@x.ResimLink" data-lightbox="portfolio"><i class="fa fa-eye"></i></a>

                            </div>
                        </div>
                    </div>
                }

            </div>

        </div>
    </div>
</div>
